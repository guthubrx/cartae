{
  "name": "@cartae/vector-store",
  "version": "1.0.0",
  "description": "Vector Store abstraction for semantic search in Cartae AI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsup src/index.ts --format esm --dts",
    "dev": "tsc --watch & tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@cartae/ai-types": "workspace:*",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
