{
  "name": "@cartae/database-api",
  "version": "0.1.0",
  "description": "Cartae Database API - PostgreSQL + pgvector persistence layer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "tsx src/db/migrations/run.ts",
    "migrate:create": "tsx src/db/migrations/create.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:perf": "tsx src/tests/performance/100k-items.test.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cartae",
    "database",
    "postgresql",
    "pgvector",
    "api",
    "embeddings",
    "vector-search"
  ],
  "author": "Cartae Project",
  "license": "MIT",
  "dependencies": {
    "@cartae/core": "workspace:*",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "node-vault": "^0.10.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node-vault": "^0.9.13",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1",
    "eslint": "^8.56.0"
  }
}
